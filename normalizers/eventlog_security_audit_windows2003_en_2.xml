                        <localized_desc language="en">The logon ID</localized_desc>
                        <localized_desc language="fr">L'identifiant de connexion</localized_desc>
                    </description>
                    <substitute>_LOGONID_</substitute>
            	</tag>
            </tags>
            <examples>
            	<example>
            		<text>A new process has been created:
New Process ID: 2167588800
Image File Name: \WINNT\system32\notepad.exe
Creator Process ID: 2167187648
User Name: administrator
Domain: ELMW2
Logon ID: (0x0,0x804C2)</text>
            		<expectedTags>
            			<expectedTag name="pid">2167588800</expectedTag>
            			<expectedTag name="file_name">\WINNT\system32\notepad.exe</expectedTag>
            			<expectedTag name="creator_pid">2167187648</expectedTag>
            			<expectedTag name="user">administrator</expectedTag>
            			<expectedTag name="domain">ELMW2</expectedTag>
            			<expectedTag name="logon_id">(0x0,0x804C2)</expectedTag>
            		</expectedTags>
            	</example>
            </examples>
        </pattern>
